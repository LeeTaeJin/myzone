

<div style="background-color:lightgray; padding-top:150px; padding-bottom:150px" id="menu_font">
  
  <div class="row">
    
    <div class="col-md-2 col-md-xs-2"></div>
      
      <div class="col-md-8 col-md-xs-8" align="center">
        <div class="panel panel-default">
          <div class="panel-body">
            
            <h2>M Y _ P A G E</h2>
            <hr>
            
            <!--<div class="col-md-2 col-md-sm-1"></div>-->
            
            <div class="col-md-4 col-sm-4">
              <div class="panel panel-default">
                <div class="panel-heading">
                  <h3 class="panel-title">회원 정보</h3>
                </div>
                <div class="panel-body">
                  
                  <table class="table" style="background:white">
                
                    <tbody>
                      
                      <tr>
                        <th>이름</th>
                        <td><%=current_user.student_name%></td>
                      </tr>
                      
                      <tr>
                        <th>학번</th>
                        <td><%=current_user.department%></td>
                      </tr>
                      
                      <tr>
                        <th>이메일</th>
                        <td><%=current_user.email%></td>
                      </tr>
                      
                    </tbody>
                  </table>
                 
                  <a href="/users/edit" class="btn btn-info btn-block" >비밀번호 변경</a>
                  <!--<a  class="btn btn-default btn-block" onclick="confirm(탈퇴하시겠습니까?)" method="destroy">회원탈퇴</a>-->
                  
                </div>
              </div>
            </div>
            
            <div class="col-md-8 col-sm-8">
              <div class="panel panel-default">
                <div class="panel-heading">
                  <h3 class="panel-title">예약 현황</h3>
                </div>
                
                <div class="panel-body">
                  <div class="row">
                    
                    <div class="col-md-4 col-sm-4" style="padding-left:20px">
                      
                      <div class="panel panel-default" ><!--총예약수-->
                        <div class="panel-body">
                          <h3><%=Reservation.where(user: current_user.id).count%></h3>
                        </div>
                        
                        <div class="panel-footer">총 예약수</div>
                      </div><!--총예약수 끝-->
                      
                    </div>
                    
                    <div class="col-md-4 col-sm-4" >
                    
                      <div class="panel panel-default"><!--승인대기-->
                        <div class="panel-body">
                          <h3><%=Reservation.where("user =? and state = ?", current_user.id, "승인대기").count%></h3>
                        </div>
                        
                        <div class="panel-footer">승인대기</div>
                      </div><!--승인대기-->
                    
                    </div>
                    
                    <div class="col-md-4 col-sm-4" style="padding-right:20px">
                      
                      <div class="panel panel-default"><!--승인완료-->
                        <div class="panel-body">
                          <h3><%=Reservation.where("user =? and state = ?", current_user.id, "승인완료").count%></h3>
                        </div>
                        
                        <div class="panel-footer">승인완료</div>
                      </div><!--승인완료 끝-->
                    
                    </div><!--div class-col-md-4-->
                  
                  <table class="table table-striped table-bordered table-hover dataTable no-footer" id="searching_table" role="grid" aria-describedby="dataTables-example_info">
                  <thead>
                    <tr role="row">
                      <th class="sorting" aria-sort="ascending" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 321.667px;">날짜</th>
                      <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 389.667px;">시간</th>
                      <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 363.667px;">강의실</th>
                      <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 283.667px;">담당 교수</th>
                      <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 220px;">상태</th>
                      <th class="sorting" tabindex="0" aria-controls="dataTables-example" rowspan="1" colspan="1" style="width: 220px;">취소</th>
                    </tr>
                  </thead>
                  <!--테이블 업데이트시 이프문을 거침(빌딩 선택 유무)-->
                  
                    <tbody>
                      <% Reservation.where(" user = ?", current_user.id).each do |r| %>  
                        <tr class="gradeA odd" role="row">
                          <td><%=r.date%></td>
                          <% @start_time= r.start_time.to_time%>
                          <% @finish_time= r.finish_time.to_time%>
                          <td><%=@start_time.hour%>시 <%=@start_time.min%>분 - <%=@finish_time.hour%>시 <%=@finish_time.min%>분</td>
                          <td><%=Room.find(r.room_id).building_name%> <%=Room.find(r.room_id).room_number%>호 </td>
                          <td><%=r.professor%></td>
                          <td><%=r.state%></td>
                          <td>
                            <div class="btn-group" role="group" aria-label="...">
                              <button type="button" class="btn btn-default btn-sm" data-toggle="modal" data-target="#delete_check<%=r.id%>">예약취소</button>
                              <%if r.state=="거절"%>
                                <button type="button" class="btn btn-info btn-sm" id="reason_show<%=r.id%>" data-toggle="modal" data-target="#reason_show<%=r.id%>">사유보기</button>
                              <%end%>
                            </div>
                              
                          </td>
                        </tr>
                      <%end%>
                    </tbody>
              
                </table>
                  
                  
                  
                  
                  </div>
                </div><!--panel body-->
              </div><!--panel panel-default-->
            </div><!--<div class="col-md-6 col-sm-6">-->
            <!--<div class="col-md-2 col-md-sm-1"></div>-->
            
              
          </div><!--<div class="panel-body">-->
        </div><!--<div class="panel panel-default">-->
      </div><!--<div class="col-md-8 col-md-xs-8">-->
      
    <div class="col-md-2 col-md-xs-2" id="menu_font">
      <%= render "book/side" %>
    </div>
    
  </div><!--div class row-->
</div><!--<div style="background-color:lightgray">-->












